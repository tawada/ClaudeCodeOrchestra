# サーバー設定
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# データベース設定
MONGODB_URI=mongodb://localhost:27017/claudecodechestra

# JWT認証設定
JWT_SECRET=your_secure_jwt_secret_key
JWT_EXPIRE=24h
JWT_COOKIE_EXPIRE=24

# セッション管理設定
MAX_CONCURRENT_SESSIONS=5
SESSION_TIMEOUT_MINUTES=120

# Git連携設定
GIT_INTEGRATION_ENABLED=false
GIT_PROVIDER=github

# CI/CD連携設定
CICD_ENABLED=false
CICD_PROVIDER=github-actions

# CORS設定
CORS_ORIGIN=*

# ロギング設定
LOG_LEVEL=info
LOG_TO_FILE=false
LOG_FILE_PATH=logs/app.log